  "version": "0.2.0",
  "configurations": [
    {
      "name": "Dart: Attach to Process",
      "type": "dart",
      "request": "attach"
    },
    {
      "type": "expo",
      "request": "attach",
      "name": "Debug Expo app",
      "projectRoot": "${workspaceFolder}",
      "bundlerPort": "8081",
      "bundlerHost": "127.0.0.1"
    },
    {
      "name": "flutter_demo001",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "flutter_demo001 (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "flutter_demo001 (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}
